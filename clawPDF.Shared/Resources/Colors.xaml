<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="TitleBarGray">#404040</Color>
    <SolidColorBrush x:Key="TitleBarGrayBrush" Color="{StaticResource TitleBarGray}" x:Shared="False" />

    <Color x:Key="ButtonGray">#6B6B6B</Color>
    <SolidColorBrush x:Key="ButtonGrayBrush" Color="{StaticResource ButtonGray}" x:Shared="False" />
</ResourceDictionary>